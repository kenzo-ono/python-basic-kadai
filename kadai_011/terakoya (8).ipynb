        "# 判定する数値を変数varに代入\n",
        "var = 15  # ←好きな正の整数に変更してOK\n",
        "\n",
        "# 条件分岐（if / elif / else）\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "    print(\"FizzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "    print(\"Buzz\")\n",
        "else:\n",
        "    print(var)\n"
